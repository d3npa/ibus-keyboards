        (";" ?æ)
        ("\\" ?*)
        ("z" ?z)
        ("x" ?x)
        ("c" ?c)
        ("v" ?v)
        ("b" ?b)
        ("n" ?n)
        ("m" ?m)
        ("," ?,)
        ("." ?.)
        ("/" ?þ)
        ("!" ?!)
        ("@" ?\")
        ("#" ?#)
        ("$" ?$)
        ("%" ?%)
        ("^" ?&)
        ("&" ?/)
        ("*" ?()
        ("(" ?))
        (")" ?=)
        ("_" ?Ö)
        ("+" ?_)
        ("Q" ?Q)
        ("W" ?W)
        ("E" ?E)
        ("R" ?R)
        ("T" ?T)
        ("Y" ?Y)
        ("U" ?U)
        ("I" ?I)
        ("O" ?O)
        ("P" ?P)
        ("{" ?Ð)
        ("}" ?\?)
        ("A" ?A)
        ("S" ?S)
        ("D" ?D)
        ("F" ?F)
        ("G" ?G)
        ("H" ?H)
        ("J" ?J)
        ("K" ?K)

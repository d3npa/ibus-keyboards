    (umlaut
        ("~" ?¨)
        ("a" ?ä)
        ("e" ?ë)
        ("i" ?ï)
        ("o" ?ö)
        ("u" ?ü)
        ("y" ?ÿ)
        ("A" ?Ä)
        ("E" ?Ë)
        ("I" ?Ï)
        ("O" ?Ö)
        ("U" ?Ü)
        ("Y" ?Ÿ))

    ;; Deadkeys
    (apostrophe ("'"))
    (backtick ("`"))
    (tilde ("~")))

(state
    (init
        (normal)
        (apostrophe (shift long_vowels))
        (backtick (shift scandinavian_a))
        (tilde (shift umlaut)))

    (long_vowels
        (long_vowels (shift init)))

    (scandinavian_a
        (scandinavian_a (shift init)))

    (umlaut
        (umlaut (shift init))))
